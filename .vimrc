execute pathogen#infect()
syntax enable
set background=dark
colorscheme solarized
set nu
filetype plugin indent on
let g:neocomplcache_enable_at_startup = 1
set tabstop=2
set shiftwidth=2
set expandtab

noremap   <Up>    ""
noremap!  <Up>    <Esc>
noremap   <Down>  ""
noremap   <Down>  <Esc>
noremap!  <Left>  ""
noremap   <Left>  <Esc>
noremap   <Right> ""
noremap!  <Right> <Esc>
